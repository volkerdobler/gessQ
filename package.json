{
  "name": "gessq",
  "displayName": "gessQ",
  "description": "gess.Q Support",
  "version": "0.3.7",
  "publisher": "volkerdobler",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/volkerdobler/gessQ.git"
  },
  "license": "AFL-3.0",
  "main": "./out/extension",
  "icon": "images/gessQ_icon.png",
  "contributes": {
    "languages": [
      {
        "id": "gessq",
        "aliases": [
          "gessQ",
          "gessq"
        ],
        "extensions": [
          ".q"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "gessq",
        "scopeName": "source.gessQ",
        "path": "./syntaxes/gessQ.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "gessq",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "prettier": "^3.5.3"
  },
  "__metadata": {
    "id": "8369ddfc-3ff3-4da4-8712-89df76ef2c89",
    "publisherId": "7c9d6a5c-7f0a-4217-8a96-4f84eae98522",
    "publisherDisplayName": "Volker Dobler"
  }
}
