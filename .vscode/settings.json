// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "out": false // set this to true to hide the "out" folder with the compiled JS files
    },
    "search.exclude": {
        "out": true // set this to false to include "out" folder in search results
    },
    // Turn off tsc task auto detection since we have the necessary tasks as npm scripts
    "typescript.tsc.autoDetect": "off",
    "cSpell.words": [
        "gess",
        "gessQ",
        "gessgroup",
        "multigridq",
        "multiq",
        "numq",
        "opennumformat",
        "openq",
        "singlegridq",
        "singleq",
        "textq"
    ],
    "workbench.colorCustomizations": {
        "[Tomorrow Night Blue]": {
            "statusBar.noFolderBackground": "#2929bb"
        },
        "[Cobalt2]": {
            "editor.findMatchHighlightBackground": "#7d8800",
            "editor.findMatchBackground": "#869e00"
        },
        "activityBar.background": "#0f360a",
        "titleBar.activeBackground": "#030b02",
        "titleBar.activeForeground": "#e7e7e7",
        "activityBar.activeBackground": "#0f360a",
        "activityBar.activeBorder": "#4354dc",
        "activityBar.foreground": "#e7e7e7",
        "activityBar.inactiveForeground": "#e7e7e799",
        "activityBarBadge.background": "#4354dc",
        "activityBarBadge.foreground": "#e7e7e7",
        "statusBar.background": "#030b02",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#0f360a",
        "titleBar.inactiveBackground": "#030b0299",
        "titleBar.inactiveForeground": "#e7e7e799",
        "sash.hoverBorder": "#0f360a",
        "statusBarItem.remoteBackground": "#030b02",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "commandCenter.border": "#e7e7e799"
    },
    "peacock.color": "#030b02"
}